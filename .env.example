# Environment Configuration Template
# Copy this file to .env and update with your actual values

# Vite WebSocket Token for HMR (Hot Module Replacement)
VITE_WS_TOKEN=your-websocket-token

# Environment Configuration
VITE_APP_NAME=GaiaExchanges
VITE_APP_VERSION=2025.1.0

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anonymous-key

# Optional: Additional Configuration
VITE_API_BASE_URL=https://api.gaiaexchanges.com
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG=false

# Note: NODE_ENV is automatically set by Vite - do not set it manually in .env files