{
  "type": "module",
  "name": "lovable-app",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "pre-deploy": "echo \"No pre-deploy steps required\"",
    "dev": "vite",
    "build": "npm run tailwind:build && vite build",
    "build:dev": "vite build --mode development",
    "build:vercel": "npx tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "lighthouse": "lighthouse http://localhost:5173 --output html --output-path ./lighthouse-report.html --chrome-flags=\"--headless\"",
    "prepare": "husky install",
    "test": "vitest run",
    "tailwind:build": "npx tailwindcss -i ./src/index.css -o ./src/tailwind.output.css --minify",
    "deploy:complete": "sh ./scripts/deploy.sh && sh ./scripts/post-deploy-validate.sh",
    "deploy:doctor": "powershell -ExecutionPolicy Bypass -File ./scripts/deployment-doctor.ps1",
    "deploy": "npm run build && npm run deploy:vercel",
    "deploy:validate": "sh ./scripts/post-deploy-validate.sh",
    "deploy:auto": "sh ./scripts/deploy.sh",
    "deploy:vercel": "npx vercel --prod --yes --archive=tgz",
    "deploy:netlify": "npx netlify deploy --prod",
    "deploy:static": "npm run build && echo 'Static build complete'"
  },
